{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:05",
      "visibilityTimeout": "00:00:10",
      "batchSize": 1,
      "maxDequeueCount": 1,
      "newBatchThreshold": 8,
      "messageEncoding": "none"
    }
  }
}